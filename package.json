{
  "name": "express-rest-api-boilerplate",
  "version": "0.0.0",
  "author": "Lukas Aichbauer <l.aichbauer@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aichbauer/express-rest-api-boilerplate.git"
  },
  "homepage": "https://github.com/aichbauer/express-rest-api-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/aichbauer/express-rest-api-boilerplate/issues"
  },
  "scripts": {
    "start": "npm run nodemon",
    "dev": "cross-env NODE_ENV=development node ./api/api.js",
    "nodemon": "nodemon --exec npm run dev",
    "lint": "eslint ./api/. ./config/. ./test/.",
    "prepush": "npm test",
    "pretest": "npm run lint",
    "production": "cross-env NODE_ENV=production node ./api/api.js",
    "test": "npm run create-sqlite-db; cross-env NODE_ENV=testing jest --coverage; npm run drop-sqlite-db",
    "test-ci": "jest --coverage --forceExit"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.2",
    "chalk": "^2.1.0",
    "core-js": "^2.6.9",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-fileupload": "^1.1.6-alpha.5",
    "express-routes-mapper": "^1.0.2",
    "express-status-monitor": "^1.0.0",
    "express-validation": "^1.0.2",
    "express-winston": "^2.4.0",
    "fs-extra": "^8.1.0",
    "helmet": "^3.12.0",
    "http-status": "^1.0.1",
    "joi": "^13.0.1",
    "jsonwebtoken": "^8.2.1",
    "multer": "^1.4.2",
    "mysql2": "^1.7.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "passport-local": "^1.0.0",
    "pretty-error": "^2.1.1",
    "random-string-generator": "^1.0.0",
    "sequelize": "^6.0.0",
    "sequelize-paginate": "^1.1.6",
    "sharp": "^0.23.1",
    "slug": "^0.9.1",
    "slugify": "^1.3.5",
    "sqlite3": "^4.0.0",
    "underscore": "^1.9.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "nodemon": "^1.17.3",
    "shx": "^0.2.2",
    "supertest": "^3.0.0"
  }
}
